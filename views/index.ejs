                    <li><i class="fas fa-map-marker-alt fa-2x"></i>
                        <p>Calicut</p>
                    </li>
    
                    <li><i class="fas fa-phone mt-4 fa-2x"></i>
                        <p>+ 01 234 567 89</p>
                    </li>
    
                    <li><i class="fas fa-envelope mt-4 fa-2x"></i>
                        <p>nuhman@bititude.com</p>
                    </li>
                </ul>
            </div>
            <div class="footer bg-secondary text-center"><p class="text-light mt-3" style="font-size: smaller">@ Copyright <%= year %>  </p></div>
